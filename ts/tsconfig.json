{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "declaration": true,
        "suppressImplicitAnyIndexErrors": true,
        "outDir": "../js"
    },
    "filesGlob": [
        "./ts/**/*.ts",
        "./ts/*.ts",
        "!./node_modules/**/*.*"
    ],
    "files": [
        "./index.ts",
        "./primitives/primitives.ts",
        "./list/array.ts",
        "./list/list.ts"
    ]
}
