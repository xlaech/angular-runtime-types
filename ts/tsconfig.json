{
    "version": "1.5.0-alpha",
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "declaration": true,
        "suppressImplicitAnyIndexErrors": true,
        "sourceMap": true,
        "outDir": "../js"
    },
    "filesGlob": [
        "./ts/**/*.ts",
        "./ts/*.ts",
        "!./node_modules/**/*.*"
    ]
}
